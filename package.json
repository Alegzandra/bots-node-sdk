{
  "name": "@oracle/bots-js-sdk",
  "version": "0.0.8",
  "description": "Oracle bot custom component service middleware and utilities",
  "main": "index.js",
  "engines": {
    "node": ">=6.11.3",
    "npm": ">=3.10.10"
  },
  "config": {
    "docdir": "docs"
  },
  "scripts": {
    "docs": "shx rm -rf ./docs && typedoc --options ./typedoc.js --exclude '**/*+(spec|schema|common)/**' --out $npm_package_config_docdir ./src/",
    "lint": "eslint . --ignore-pattern reports",
    "test": "istanbul cover ./spec/support/test.js",
    "prepack": "npm run lint"
  },
  "contributors": [
    "Matt Vander Vliet <matthew.vander.vliet@oracle.com>",
    "Adao Oliveira Junior <adao.junior@oracle.com>",
    "Henrick Yau <henrick.yau@oracle.com>"
  ],
  "license": "UPL-1.0",
  "files": [
    "config",
    "util",
    "middleware",
    "testing",
    "Dockerfile",
    ".dockerignore",
    "LICENSE.md"
  ],
  "dependencies": {
    "body-parser": "^1.18.2",
    "joi": "^13.0.2",
    "leven": "^2.1.0",
    "request": "^2.81.0"
  },
  "devDependencies": {
    "eslint": "^4.18.1",
    "eslint-plugin-jasmine": "^2.9.2",
    "express": "^4.15.4",
    "istanbul": "^0.4.5",
    "jasmine": "^2.8.0",
    "jasmine-reporters": "^2.2.1",
    "jasmine-spec-reporter": "^4.2.1",
    "log4js": "^2.3.4",
    "remap-istanbul": "^0.9.5",
    "shx": "^0.2.2",
    "supertest": "^3.0.0",
    "tslint": "^5.7.0",
    "typedoc": "^0.9.0",
    "typescript": "2.5.2"
  },
  "peerDependencies": {
    "express": "^4.15.4"
  }
}
